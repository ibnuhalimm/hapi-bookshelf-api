{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": [ "error", 4 ],
        "no-console": "off",
        "no-unused-vars": "off",
        "max-len": 0,
        "object-curly-spacing": ["error", "always"]
    }
}
